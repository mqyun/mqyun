.maxLoadingTip
  position fixed
  top 0
  left 0
  width 100%
  height 100%
  z-index 9999
  background-color rgba(0, 0, 0, .1)

.wrapLoadingTip
  position absolute
  top 0
  left 0
  width 100%
  height 100%
  z-index 9999
  background-color rgba(0, 0, 0, .1)

.maxLoadingTip img,
.wrapLoadingTip img
  position absolute
  top 50%
  left 50%

.maxLoadingTip img
  width 42px
  height 42px
  margin-left -21px
  margin-top -21px

.wrapLoadingTip img
  width 22px
  height 22px
  margin-left -11px
  margin-top -11px